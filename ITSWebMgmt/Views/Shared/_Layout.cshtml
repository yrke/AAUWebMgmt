</head>
<body>
    <header>
        <div class="ui fixed inverted menu">
            <div class="ui container">
                <div href="#" class="header item">
                    <img class="logo" src="http://www.resources.aau.dk/design2013/images/logo.png">
                    ITS Webmgmt
                </div>
                <a class="item" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                <div href="#" class="ui simple dropdown item">
                    Tools <i class="dropdown icon"></i>
                          <div class="menu">
                              <a class="item" asp-area="" asp-controller="User" asp-action="Index">User Info</a>
                              <a class="item" asp-area="" asp-controller="Computer" asp-action="Index">Computer Info</a>
                              <a class="item" asp-area="" asp-controller="DefendpointChallengeResponse" asp-action="Index">Defendpoint</a>
                          </div>
                </div>
                <a href="onenote:https://docs.its.aau.dk/Documentation/SupportDokumentation" class="item">Dokumentation (onenote)</a>
            </div>
        </div>
    </header>

    <div class="ui main text container">
        <partial name="_CookieConsentPartial" />
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @RenderSection("Scripts", required: false)
    <footer class="border-top footer text-muted">
        <div class="ui inverted vertical footer segment">
            <div class="ui center aligned container">
                <img src="http://www.resources.aau.dk/design2013/images/logo.png" class="ui centered mini image">
                <p>&copy; @DateTime.Now.Year - ITS Web Manager by kyrke</p>
            </div>
        </div>
    </footer>
</body>
</html>
